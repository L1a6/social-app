{
  "cli": {
    "version": ">= 13.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "cache": {
        "disabled": false
      },
      "env": {
        "NODE_ENV": "production"
      }
    },
    "production": {
      "cache": {
        "disabled": false
      }
    }
  },
  "submit": {
    "production": {}
  }
}
